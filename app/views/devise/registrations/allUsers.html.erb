<h1>All Users</h1>
<table>
  <thead>
    <tr>
      <th>Full Name</th>
      <th>Email</th>
      <th>Admin?</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.full_name %></td>
        <td><%= user.email %></td>
        <td><%= user.is_admin? ? 'Yes' : 'No' %></td>
      </tr>
    <% end %>
  </tbody>
</table>
